<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    
    <!-- Semantic Versioning -->
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
    <Version>1.0.0</Version>
    
    <!-- Application Information -->
    <AssemblyTitle>Turbo Math Rally</AssemblyTitle>
    <AssemblyDescription>A complete educational rally racing math game for ages 5-12. Features instant gameplay, live statistics, and professional polish.</AssemblyDescription>
    <AssemblyCompany>George Aaron Kibbie</AssemblyCompany>
    <AssemblyProduct>Turbo Math Rally</AssemblyProduct>
    <Copyright>Copyright © 2025 George Aaron Kibbie</Copyright>
  </PropertyGroup>

</Project>
